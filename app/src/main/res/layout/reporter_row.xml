<?xml version="1.0" encoding="utf-8"?>
<TableRow xmlns:android="http://schemas.android.com/apk/res/android"
          android:layout_width="match_parent"
          android:layout_height="wrap_content">
  <TextView android:id="@+id/registration_activation_time"
      android:gravity="end"
      android:layout_marginRight="14dp"
      android:layout_marginEnd="14dp"
      android:textSize="14sp" />
  <TextView android:id="@+id/registration_label"
      android:layout_marginRight="14dp"
      android:layout_marginEnd="14dp"
      android:textSize="14sp" />
  <TextView android:id="@+id/registration_number"
      android:layout_marginRight="14dp"
      android:layout_marginEnd="14dp"
      android:textSize="14sp" />
  <Button android:id="@+id/details"
      android:minHeight="44sp"
      android:minWidth="80sp"
      android:textSize="14sp"
      android:text="Details" />
</TableRow>
